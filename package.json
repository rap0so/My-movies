{
  "name": "micro-frontend-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "eject": "react-scripts eject",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "start": "react-scripts start",
    "test": "react-scripts test --passWithNoTests --watchAll=false",
    "test:cov": "react-scripts test --coverage --passWithNoTests --watchAll=false",
    "test:watch": "react-scripts test",
    "test:e2e:push": "PORT=4100 start-server-and-test start 4100 cy:run",
    "test:e2e": "PORT=4100 start-server-and-test start 4100 cy:open"
  },
  "dependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.5.0",
    "lint-staged": "^10.5.3",
    "loglevel": "^1.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-spinners": "^0.10.4",
    "react-toastify": "^7.0.2",
    "rebass": "^4.0.7",
    "start-server-and-test": "^1.11.7",
    "theme-ui": "^0.3.5",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/rebass": "^4.0.7",
    "@types/reflexbox": "^4.0.2",
    "@types/styled-components": "^5.1.7",
    "@types/theme-ui": "^0.3.7",
    "cypress": "^6.3.0",
    "husky": "^4.3.8",
    "prettier": "^2.2.1",
    "source-map-explorer": "^2.5.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  }
}
