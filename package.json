{
  "name": "micro-frontend-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "cy:run": "cypress run",
    "eject": "react-scripts eject",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --project tsconfig.json --fix",
    "start": "react-scripts start",
    "test": "react-scripts test --passWithNoTests --watchAll=false",
    "test:cov": "react-scripts test --coverage --passWithNoTests --watchAll=false",
    "test:watch": "react-scripts test",
    "test:e2e": "PORT=4200 start-server-and-test start 4200 cy:run"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-spinners": "^0.9.0",
    "reflexbox": "^4.0.6",
    "start-server-and-test": "^1.11.2",
    "styled-components": "^5.1.1",
    "typescript": "3.9.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/reflexbox": "^4.0.1",
    "@types/styled-components": "^5.1.1",
    "cypress": "^4.10.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "source-map-explorer": "^2.4.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-prettier": "^1.1.2",
    "tslint": "^6.1.2",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "tslint-react": "^5.0.0"
  }
}
